{
  "name": "eevee-client-web",
  "version": "2.44.0",
  "main": "firebaseFunctionsSTG.js",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prepare": "./prepare.sh",
    "dev": "next dev & tsc --watch",
    "dev:stg": "APP_ENV=staging npm run dev",
    "dev:ssl": "APP_ENV=staging next dev & local-ssl-proxy --config local-ssl-proxy.config.json",
    "build": "next build",
    "build:stg": "APP_ENV=staging npm run build",
    "build:prod": "APP_ENV=production npm run build",
    "build:function": "FUNCTIONS=true APP_ENV=staging npm run build",
    "start": "serve out",
    "lint": "eslint --cache 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "stylelint": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint --fix 'src/**/*.{css,scss}'",
    "prettier": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,scss,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss,json}'",
    "codegen": "graphql-codegen --config codegen.yml && eslint --fix 'src/**/*.generated.*' 'src/graphql/generated/*'  && prettier --write 'src/**/*.generated.*' 'src/graphql/generated/*'",
    "codegen:theme-typings": "chakra-cli tokens ./src/theme/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "vitest --update",
    "bundle-analyzer": "APP_ENV=production ANALYZE=true next build",
    "clean:function": "rm -rf ./functions/node_modules && rm -rf ./functions/.next",
    "emulate": "npm run clean:function && npm run build:function && firebase emulators:start --only functions,hosting:eevee-client-web-dev",
    "deploy:stg": "firebase deploy --project staging --only functions:eeveeClientWebStaging,hosting:eevee-client-web-dev",
    "deploy:stg2": "firebase deploy --project staging --only functions:eeveeClientWebStaging2,hosting:eevee-client-web-dev2",
    "deploy:stg3": "firebase deploy --project staging --only functions:eeveeClientWebStaging3,hosting:eevee-client-web-dev3",
    "deploy:prod": "npm run build:prod && firebase deploy --project production --only hosting:eevee-client",
    "deploy:prod-test": "npm run build:prod && firebase hosting:channel:deploy --project production --only eevee-client production-test",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.10.3",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.1",
    "@line/liff": "^2.24.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@react-google-maps/api": "^2.20.3",
    "@sentry/nextjs": "^8.34.0",
    "@stripe/react-stripe-js": "^2.8.1",
    "@stripe/stripe-js": "^4.8.0",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-auth": "^2.2.0",
    "express": "^4.21.2",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "firebase-tools": "^13.22.1",
    "focus-visible": "^5.2.1",
    "framer-motion": "^11.11.9",
    "graphql": "^16.9.0",
    "graphql-anywhere": "^4.2.8",
    "graphql-tag": "^2.12.6",
    "next": "14.2.15",
    "passport": "^0.7.0",
    "passport-http": "^0.3.0",
    "qrcode.react": "^4.0.1",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-is": "^18.3.1",
    "react-swipeable": "^7.0.1",
    "urql": "^4.1.0",
    "valibot": "^0.42.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.5.5",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "4.1.0",
    "@graphql-codegen/typescript-operations": "4.3.0",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@next/bundle-analyzer": "^14.2.15",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/node": "22.7.6",
    "@types/react": "18.3.11",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-infinite-scroller": "^1.2.5",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "firebase-functions-test": "^3.3.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "local-ssl-proxy": "^2.0.5",
    "prettier": "^3.3.3",
    "sass": "^1.80.1",
    "serve": "^14.2.4",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-mercari": "^0.14.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^5.3.2",
    "typescript": "~5.5.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "vitest": "^2.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{css,scss}": "stylelint --fix",
    "*.{js,jsx,ts,tsx,css,scss,json,yml}": "prettier --write"
  }
}
